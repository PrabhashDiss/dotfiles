case ":$PATH:" in
  *":$HOME/bin:") ;; 
  *) PATH="$HOME/bin:$PATH" ;;
esac

case ":$PATH:" in
  *":$HOME/.local/bin:") ;; 
  *) PATH="$HOME/.local/bin:$PATH" ;;
esac

. "$HOME/.cargo/env"

. "$HOME/.local/share/../bin/env"

case ":$PATH:" in
  *":$HOME/.opencode/bin:") ;; 
  *) PATH="$HOME/.opencode/bin:$PATH" ;;
esac

export PATH
